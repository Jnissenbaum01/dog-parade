<div class="container-fluid bg-dark">
  <div class="text-center">
    <h1 class="text-primary"> Dog Viewer </h1>
    <h5><span class="bg-info">Current Dog: <%= currentDog.name %> (<%=currentDog.index%>)</span></h5>
    <p><span class="badge badge-secondary"><a href="<%=currentDog.url%>">[Wikipedia Article]</a></span></p>

    <!-- Main Dog Photo-->
    <!-- <div>
      <% if (currentDogPhotoURL) { %>
        <img src="<%=currentDogPhotoURL%>" class="mr-3 w-75 " alt="...">
      <% } else { %>
        <img src="images/dogicon.png" class="mr-3 w-50 " alt="...">
      <% } %>
    </div> -->

    <!-- Carousel -->
    <% let LEN = extraImages.length %>
    <div class="container w-75 h-25">
      <div id="extraImagesCarousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#extraImagesCarousel" data-slide-to="0" class="active"></li>
          <% for (let i=0; i<LEN; i++){%>
            <li data-target="#extraImagesCarousel" data-slide-to="<%=1+i%>"></li>
          <%}%>
        </ol>

        <div class="carousel-inner">
          <div class="carousel-item active">
            <img src="<%=currentDogPhotoURL%>" class="mw-100 mh-100" alt="0">
          </div>
          <% for (let i=0; i<LEN; i++){%>
            <div class="carousel-item">
              <img src="<%=extraImages[i]%>" class="mw-100 mh-100" alt="<%="ei"+i%>">
            </div>
          <%}%>
        </div>

        <a class="carousel-control-prev" href="#extraImagesCarousel" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#extraImagesCarousel" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>

  <br>
  </div>
</div>
